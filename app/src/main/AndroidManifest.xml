<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mankomapps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logomankom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MankomApps">
        <activity android:name=".Ukm"></activity>
        <activity android:name=".HimpunanMahasiswa" />
        <activity android:name=".MataKuliah" />
        <activity android:name=".Fasilitas" />
        <activity android:name=".Tujuan" />
        <activity android:name=".StafProdi" />
        <activity android:name=".Struktur" />
        <activity android:name=".Akreditasi" />
        <activity android:name=".VisiMisi" />
        <activity android:name=".Sejarah" />
        <activity android:name=".ProfilLulusan" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>